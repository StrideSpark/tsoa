{
    "compilerOptions": {
        "declaration": true,
        "declarationMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "outDir": "dist",
        "skipLibCheck": true,
        "sourceMap": true,
        "strictBindCallApply": true,
        "strictNullChecks": true,
        "target": "es2017",
    },
    // "exclude": [
    //     "dist/**"
    // ],
    "include": [
        "src/**/*.ts"
    ]
}
